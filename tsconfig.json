{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@/*": [
        "./*"
      ],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
